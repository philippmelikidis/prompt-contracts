{
  "checks": [
    {
      "type": "json_valid",
      "severity": "error"
    },
    {
      "type": "json_required",
      "severity": "error",
      "keys": ["summary", "length"]
    },
    {
      "type": "latency_budget",
      "severity": "warn",
      "max_ms": 5000
    },
    {
      "type": "token_budget",
      "severity": "warn",
      "max_tokens": 250
    }
  ]
}
