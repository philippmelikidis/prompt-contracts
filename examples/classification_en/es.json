{
  "output_schema": {
    "type": "object",
    "properties": {
      "intent": {
        "type": "string",
        "enum": ["cancel", "upgrade", "inquiry", "refund", "support", "info", "sales", "access", "pricing", "complaint"]
      },
      "confidence": {
        "type": "number",
        "minimum": 0.0,
        "maximum": 1.0
      }
    },
    "required": ["intent", "confidence"]
  }
}
